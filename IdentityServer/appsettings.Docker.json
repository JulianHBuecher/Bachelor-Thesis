{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.ms} {Level:u3}] {ThreadId} {MachineName} {EnvironmentUserName} {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "Elasticsearch",
        "Options": [
          {
            "AutoRegisterTemplate": "true"
          }
        ],
        "Args": {
          "nodeUris": "http://elasticsearch:9200",
          "indexFormat": "logstash-{0:yyyy.MM.dd}"
        }
      }
    ]
  },
  "ProxyHeader": "X-Forwarded-For",
  "InternalBasePath": "/",
  "ProxyBasePath": "/identity",

  "WebApp": {
    "Callback-Url": "https://reverse.proxy.localhost/",
    "Silent-Refresh-Url": "https://reverse.proxy.localhost/silent-renew.html",
    "Front-Channel-Logout-Url": "https://reverse.proxy.localhost/",
    "Post-Logout-Redirect-Url": "https://reverse.proxy.localhost/",
    "Additional-Logout-Redirect-Url": "https://reverse.proxy.localhost/unauthorized",
    //"Callback-Url": "https://localhost/",
    //"Silent-Refresh-Url": "https://localhost/silent-renew.html",
    //"Front-Channel-Logout-Url": "https://localhost/",
    //"Post-Logout-Redirect-Url": "https://localhost/",
    //"Additional-Logout-Redirect-Url": "https://localhost/unauthorized"
  }
}